<%@ page language="java" contentType="text/html; charset=UTF-8" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>SECO</title>
</head>
<body>
<h1>店舗／ユーザー登録</h1>
<p>
<a href="/JS/Logout">ログアウト</a>
</p>
<form action="/JS/Seco_store" method="post">
アカウント名　　<input type="text" name="userID">　　
パスワード<input type="password" name="pass"><br>
店舗名／氏名　　<input type="text" name="userName"><br>
郵便番号　　　　<input type="text" name="postCode"><br>
住所　　　　　　<input type="text" name="address"><br>
電話番号　　　　<input type="text" name="phone"><br>
店舗コメント　　<input type="text" name="shopComment"><br>
メールアドレス　<input type="text" name="mail"><br>
<input type="submit" value="登録">
</form>
<c:if test="${not empty errorMsg }">
	<p>${errorMsg}</p>
</c:if>
<c:forEach var="secoStoreB" items="${secoStoreBList}">
	<p><c:out value="${secoStoreB.userID}" />
		<c:out value="${secoStoreB.mail}" /></p>
</c:forEach>
</body>
</html>